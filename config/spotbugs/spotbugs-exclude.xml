<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Exclude generated code -->
    <Match>
        <Package name="~.*\.generated\..*"/>
    </Match>

    <!-- Exclude test code from certain checks -->
    <Match>
        <Class name="~.*Test.*"/>
        <Bug pattern="DM_EXIT"/>
    </Match>
</FindBugsFilter>
